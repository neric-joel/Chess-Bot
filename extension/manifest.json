{
    "manifest_version": 3,
    "name": "Chess Cheater",
    "version": "1.0",
    "description": "This extension will help you cheat in chess",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["socket.io.min.js", "content.js"]
        }
    ],
    "host_permissions": [
        "http://localhost:5000/*"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}